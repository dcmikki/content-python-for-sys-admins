#!/usr/bin/env python3.6

user = { 'admin': False, 'active': True, 'name': 'Maria' }
prefix = f"{user['name']}:\t "

if user['admin'] and user['active']:
    prefix += "(ACTIVE) - (ADMIN)"

elif user['admin']:
    prefix += "(ADMIN)"

elif user['active']:
    prefix += "(ACTIVE)"

elif user['name']:
    prefix += "No privilegios"

elif len(user['name']) == 0:
    prefix += "No has escrito ningun nombre"
print(prefix)

